{
  "name": "revendas-adianti",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "workspaceFolder": "/app/",
  "service": "webserver-revenda",
  "customizations": {
    "vscode": {
      "extensions": [
        "adpyke.codesnap",
        "anseki.vscode-color",
        "bierner.color-info",
        "bradymholt.pgformatter",
        "chrisdias.vscode-opennewinstance",
        "christian-kohler.path-intellisense",
        "donjayamanne.githistory",
        "dracula-theme.theme-dracula",
        "dudemelo.php-essentials",
        "eamodio.gitlens",
        "ecodes.vscode-phpmd",
        "editorconfig.editorconfig",
        "equinusocio.vsc-material-theme-icons",
        "expo.vscode-expo-tools",
        "formulahendry.auto-rename-tag",
        "formulahendry.docker-explorer",
        "formulahendry.docker-extension-pack",
        "futureglobe.sepaswitch",
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "jock.svg",
        "mechatroner.rainbow-csv",
        "meganrogge.template-string-converter",
        "mehedidracula.php-constructor",
        "mhutchie.git-graph",
        "mohd-akram.vscode-html-format",
        "ms-azuretools.vscode-docker",
        "ms-ceintl.vscode-language-pack-pt-br",
        "ms-kubernetes-tools.vscode-kubernetes-tools",
        "ms-vscode-remote.remote-containers",
        "ms-vscode-remote.remote-ssh",
        "ms-vscode-remote.remote-ssh-edit",
        "ms-vscode-remote.remote-wsl",
        "ms-vscode.remote-explorer",
        "ms-vscode.remote-server",
        "ms-vscode.test-adapter-converter",
        "ms-vscode.vscode-typescript-next",
        "ms-vsliveshare.vsliveshare",
        "mtxr.sqltools",
        "mtxr.sqltools-driver-mysql",
        "mtxr.sqltools-driver-pg",
        "mtxr.sqltools-driver-sqlite",
        "naumovs.color-highlight",
        "negokaz.live-server-preview",
        "oderwat.indent-rainbow",
        "pdconsec.vscode-print",
        "pkief.material-icon-theme",
        "recca0120.vscode-phpunit",
        "redhat.vscode-xml",
        "redhat.vscode-yaml",
        "ritwickdey.liveserver",
        "streetsidesoftware.code-spell-checker",
        "vmware.vscode-manifest-yaml",
        "wongjn.php-sniffer",
        "xdebug.php-debug",
        "zobo.php-intellisense"
      ],
      "settings": {
        "yaml.schemas": {
          "file:///home/lucas/.vscode/extensions/atlassian.atlascode-3.2.1/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
        },
        "workbench.iconTheme": "material-icon-theme",
        "security.workspace.trust.untrustedFiles": "open",
        "git.confirmSync": false,
        "redhat.telemetry.enabled": false,
        "[json]": {
          "editor.formatOnSave": false,
          "editor.defaultFormatter": "vscode.json-language-features"
        },
        "git.autofetch": true,
        "workbench.editorAssociations": {
          "*.json": "default",
          "*.db": "default",
          "*.wt": "default",
          "*.sql": "default",
          "*.pfx": "default",
          "*.gz": "default"
        },
        "editor.formatOnSave": true,
        "typescript.updateImportsOnFileMove.enabled": "always",
        "diffEditor.ignoreTrimWhitespace": false,
        "workbench.startupEditor": "none",
        "gitlens.gitCommands.skipConfirmations": [
          "fetch:command",
          "stash-push:command",
          "switch:command",
          "branch-create:command"
        ],
        "gitlens.advanced.messages": {
          "suppressCreatePullRequestPrompt": true
        },
        "[xml]": {
          "editor.defaultFormatter": "redhat.vscode-xml"
        },
        "editor.fontLigatures": true,
        "editor.fontFamily": "'Fira Code'",
        "terminal.integrated.fontFamily": "monospace",
        "javascript.updateImportsOnFileMove.enabled": "always",
        "liveServer.settings.donotShowInfoMsg": true,
        "terminal.integrated.defaultProfile.windows": "Git Bash",
        "material-icon-theme.activeIconPack": "nest",
        "explorer.confirmDragAndDrop": false,
        "explorer.compactFolders": false,
        "material-icon-theme.folders.associations": {
          "adapters": "contract",
          "grpc": "pipe",
          "kube": "kubernetes",
          "main": "lib",
          "websockets": "pipe",
          "implementations": "core",
          "protos": "pipe",
          "entities": "class",
          "kafka": "pipe",
          "use-cases": "functions",
          "migrations": "tools",
          "schemas": "class",
          "useCases": "functions",
          "eslint-config": "tools",
          "typeorm": "database",
          "_shared": "shared",
          "mappers": "meta",
          "fakes": "mock",
          "modules": "components",
          "subscribers": "messages",
          "domain": "class",
          "protocols": "contract",
          "infra": "app",
          "view-models": "views",
          "presentation": "template",
          "dtos": "typescript",
          "http": "container",
          "providers": "include",
          "factories": "class",
          "repositories": "mappings"
        },
        "yaml.customTags": [
          "!And",
          "!And sequence",
          "!If",
          "!If sequence",
          "!Not",
          "!Not sequence",
          "!Equals",
          "!Equals sequence",
          "!Or",
          "!Or sequence",
          "!FindInMap",
          "!FindInMap sequence",
          "!Base64",
          "!Join",
          "!Join sequence",
          "!Cidr",
          "!Ref",
          "!Sub",
          "!Sub sequence",
          "!GetAtt",
          "!GetAZs",
          "!ImportValue",
          "!ImportValue sequence",
          "!Select",
          "!Select sequence",
          "!Split",
          "!Split sequence"
        ],
        "files.associations": {
          "*.php": "php",
          "*.yml": "yaml"
        },
        "editor.codeActionsOnSave": {
          "source.organizeImports": "explicit"
        },
        "git.openRepositoryInParentFolders": "never",
        "editor.guides.bracketPairs": true,
        "editor.bracketPairColorization.enabled": false,
        "editor.tabSize": 2,
        "files.exclude": {
          "**/.git": false
        },
        "remote.autoForwardPortsSource": "hybrid",
        "workbench.settings.applyToAllProfiles": [
          "prettier.singleQuote"
        ],
        "git.suggestSmartCommit": false,
        "extensions.ignoreRecommendations": true,
        "terminal.integrated.env.linux": {},
        "terminal.integrated.env.windows": {},
        "[html]": {
          "editor.defaultFormatter": "mohd-akram.vscode-html-format"
        },
        "gitlens.views.branches.branches.layout": "list",
        "editor.maxTokenizationLineLength": 2000000,
        "[dockerfile]": {
          "editor.defaultFormatter": "ms-azuretools.vscode-docker"
        },
        "liveServer.settings.donotVerifyTags": true,
        "git.ignoreRebaseWarning": true,
        "[sql]": {
          "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
        },
        "svg.preview.mode": "svg",
        "gitlens.telemetry.enabled": false,
        "github.copilot.enable": {
          "*": true,
          "plaintext": false,
          "markdown": false,
          "scminput": false
        }
      }
    }
  }
}